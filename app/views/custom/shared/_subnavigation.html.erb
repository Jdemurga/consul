<div class="small-12 medium-8 column subnavigation-menu">
  <ul class="dropdown menu" data-dropdown-menu>
    <% if feature?(:budgets) %>
      <li>
        <%= link_to t("layouts.header.participatory_budgets"), budgets_path, accesskey: "p" %>
      </li>
    <% end %>
    <% if feature?(:legislation) %>
      <li>
        <%= layout_menu_link_to t("layouts.header.collaborative_legislation"),
                                legislation_processes_path,
                                controller.class.parent == Legislation,
                                accesskey: "l" %>
      </li>
    <% end %>
    <li>
      <%= link_to t("layouts.header.proposals"), proposals_path, class: ("active" if current_page?(proposals_path)), accesskey: "p" %>
    </li>
    <li>
      <%= link_to t("layouts.header.previous_hearing"), "https://transparencia.getafe.es/portalGetafe/transparencia/se_contenedor2.jsp?seccion=s_fdes_d4_v2.jsp&codbusqueda=207&language=es&codResi=1&codMenuPN=107&codMenuSN=117&codMenuTN=297&codMenu=318", target: '_blank' %>
    </li>

      <li>
        <a class="subnavigation-submenu-link"><%= t("layouts.header.assemblies") %></a>
        <ul class="menu subnavigation-submenu-item" style="top:42px">
          <% assembly_types_for_menu.each do |assembly_type| %>
            <li><%= link_to assembly_type.name, assemblies.assemblies_path(assembly_type_id: assembly_type.id), class: 'subnavigation-a-width' %></li>
          <% end %>
          <li><a href="/associative_spaces"><%= t("layouts.header.model") %></a></li>
          <li><a href="/volunteerings"><%= t("layouts.header.volunteering") %></a></li>
        </ul>
      </li>

  </ul>
</div>
